//Problem Link: https://leetcode.com/problems/jump-game/

class Solution {
public:
    bool canJump(vector<int>& nums) {
        int target=0, n=nums.size();
        for(int i=0;i<n;i++) {
            if(i>target)
                return false;
            target=max(target, i+nums[i]);
        }
        return true;
    }
};
