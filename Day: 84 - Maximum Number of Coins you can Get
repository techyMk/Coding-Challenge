//Problem Link: https://leetcode.com/problems/maximum-number-of-coins-you-can-get/

class Solution {
public:
    int maxCoins(vector<int>& piles) {
        sort(piles.begin(), piles.end());
        int n=piles.size(), res=0, k=n/3;
        for(int i=0;i<k;++i) {
            res+=piles[n-2-2*i];
        }
        return res;
    }
};
