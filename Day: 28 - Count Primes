//Problem Link: https://leetcode.com/problems/count-primes/

class Solution {
public:
    int countPrimes(int n) {
        int count=0;
        vector<int> check(n+1, 1);
        for(int i=2;i<n;i++) {
            if(check[i]==1) {
                count++;
                for(long long j=(long long)i*i;j<n;j+=i) {
                    check[j]=0;
                }
            }
        }
        return count;
    }
};
